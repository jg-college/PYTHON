num=int(input("Enter a number:"))
print("odd" if num % 2 !=0 else "even")

for i in range(1,11):
    print(i)

n=1
while n<=5:
    print("Hello")
    n+=1

age=int(input("Enter your age:"))
if age<13:
    print("Child")
elif age<20 and age>13:
    print("Teen")
else:
    print("Adult")
    
lst=[10,20,30,40,50]
for i in lst:
    print(i)
